<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="mobi.garden.bottomnavigationtest.Activity.HistoryActivity"
    >

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/gradient"
        >

        <include
            android:id="@+id/include"
            layout="@layout/user_search"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true" />

    </RelativeLayout>

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/grup_header"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:text="Pilih Kategori"
                android:textColor="@color/black"/>

            <TextView
                android:id="@+id/AlatKesehatan"
                android:layout_width="381dp"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Alat Kesehatan"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/AlergidanSistemImun"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Alergi Dan Sistem Imun"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/BayiDanAnak"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Bayi Dan Anak"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/Darah"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Darah"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/DemamDanNyeri"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Demam Dan Nyeri"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/Diabetes"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Diabetes"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/GangguanTidur"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Gangguan Tidur"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/GiziDanNutrisi"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Gizi Dan Nutrisi"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/HamilDanMenyusui"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Hamil Dan Menyusui"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>

            <TextView
                android:id="@+id/KosmetikDanKecantikan"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/green1"
                android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
                android:text="Kosmetik Dan Kecantikan"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_marginBottom="3dp"/>
        </LinearLayout>

    </FrameLayout>

    <include
        layout="@layout/element_bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        />

</LinearLayout>
